.bg_box {
  width: 100%;
  height: 100%;
}
.bg_box image {
  position: fixed;
  width: 100%;
  height: 100%;
}
.content_box{
  width: 90%;
  position: absolute;
  left: 5%;
}
.code{
  margin-top: 50rpx;
  margin-bottom: 10rpx;
  position: relative;
 }
 .get_back{
  position: relative;
  top: 200rpx;
}
.photo image{
  position: relative;
  left: 130rpx;
  width: 50%;
  height: 50%;
} 
.pop_bg {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 66;
}
 
.pop_box {
  width: 630rpx;
  background: #fff;
  padding-top: 40rpx;
  position: absolute;
  left: 60rpx;
  top: 200rpx;
  border-radius: 10rpx;
  color: #333;
}
 
.pop_box .title {
  font-size: 32rpx;
  color: #333;
  font-weight: 600;
  text-align: center;
}
 
.pop_box input {
  border: 1rpx solid #bfbfbf;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  padding-left: 20rpx;
  margin: 40rpx;
    text-align: center;
}
 
.inputholder {
  color: #a9a9a9;
  text-align: center;
}
 
.btn_box {
  display: flex;
  justify-content: space-around;
  border-top: 1rpx solid #efeff4;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
}
 
.btn_cancel {
  flex: 1;
  text-align: center;
  border-right: 1rpx solid #efeff4;
}
 
.btn_sub {
  flex: 1;
  text-align: center;
  color: #1aad19;
}